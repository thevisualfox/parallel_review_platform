<!DOCTYPE html>
<html class="no-js" lang="nl">
    <head>
        <meta charset="utf-8">
        <meta name="author" content="Parallel Realities">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="{{ asset('build/app/manifest.json') }}" rel="manifest">
        <title>Parallel Dashboard | {% block title %}{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('main') }}
        {% endblock %}
    </head>
    <body>
        {% block header %}
            {% include "compositions/main-header.html.twig" %}
        {% endblock %}
        {% block content %}{% endblock %}
        {% block modals %}{% endblock %}
        {% block javascripts %}
           {{ encore_entry_script_tags('main') }}
        {% endblock %}
    </body>
</html>
